    "sns.set_palette('RdBu_r')\n",
    "sns.set_style('darkgrid')\n",
    "sns.catplot(x='Year', y='Birth Rate', data=df, kind='bar')\n",
    "plt.xticks(rotation=90)\n",
    "plt.show()\n",
    "# what happene to year 2010?"
